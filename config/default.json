{
    "model": {
        "default_model": "cpu_small",
        "model_path": "./models/",
        "cache_size": 1024,
        "inference_threads": 4
    },
    "system": {
        "monitor_interval": 5,
        "resource_threshold": {
            "cpu": 80.0,
            "memory": 85.0,
            "disk": 90.0
        },
        "auto_optimize": true
    },
    "logging": {
        "level": "info",
        "file": "./logs/aicli.log",
        "max_size": "10MB",
        "max_files": 5
    },
    "cli": {
        "interactive_mode": true,
        "color_output": true,
        "command_history": true
    }
}
